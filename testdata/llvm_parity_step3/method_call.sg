type Point = {
    x: int,
    y: int,
};

extern<Point> {
    fn __add(self: &Point, other: &Point) -> Point {
        return { x: self.x + other.x, y: self.y + other.y };
    }

    fn sum(self: &Point) -> int {
        return self.x + self.y;
    }
}

@entrypoint
fn main() -> int {
    let p1: Point = { x: 1, y: 2 };
    let p2: Point = { x: 3, y: 4 };
    let p3: Point = p1 + p2;
    let total: int = p3.sum();
    if total == 10 {
        print("ok");
    } else {
        print("bad");
    }
    return 0;
}
