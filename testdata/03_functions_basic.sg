// Базовые функции и параметры
fn no_params() {
    // Функция без параметров и возвращаемого значения
}

fn with_return() -> int {
    return 42;
}

fn add_two_numbers(a: int, b: int) -> int {
    return a + b;
}

fn multiple_params(x: int, y: float, msg: string) -> string {
    return msg;
}

// Функция с мутабельными параметрами
fn modify_value(mut x: int) -> int {
    x = x + 10;
    return x;
}

// Variadic функция
fn print_many(...args) {
    // В будущем: печать всех аргументов
}

fn test_function_calls() -> int {
    no_params();

    let result1: int = with_return();
    let result2: int = add_two_numbers(10, 20);
    let msg: string = multiple_params(1, 2.5, "test");
    let modified: int = modify_value(5);

    print_many(1, 2, 3, "hello");

    return result1 + result2 + modified;
}