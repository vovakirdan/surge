// Examples of incorrect extern field declarations and expected diagnostics.

type Broken = {}

extern<Broken> {
    field id: int;
    // Duplicate field name for the same target type: triggers SemaExternDuplicateField
    @override field id: int;
}

extern<Broken> {
    // Modifiers are still reserved for functions; `pub` before a field is illegal in extern blocks.
    pub field shadow: string;
}
