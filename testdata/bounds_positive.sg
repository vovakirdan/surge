// Examples of type parameter bounds in functions and types.

fn f<T: FooLike>(t: T) -> int;
fn f_long<T: FooLike<T>>(t: T) -> int;
fn g<T: BarLike<T, U>, U>();
fn h<T: Readable + Serializable<T>>();
fn k<T: X + Y<T> + Z<T, U>, U>();

type List<T: Iterable<T>> = {};
type Pair<T: Eq, U: Ord<T>> = {};
