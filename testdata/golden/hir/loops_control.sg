fn find_first_positive(arr: int[5]) -> int {
    for i: int in 0..5 {
        if arr[i] < 0 {
            continue;
        }
        return arr[i];
    }
    return 0;
}

fn sum_until_zero(arr: int[5]) -> int {
    let mut total: int = 0;
    for i: int in 0..5 {
        if arr[i] == 0 {
            break;
        }
        total = total + arr[i];
    }
    return total;
}
