// scenario: user-defined __to for struct -> string

type Point = {
    x: int,
    y: int,
}

extern<Point> {
    fn __to(self: Point, _: string) -> string {
        return "Point(" + self.x to string + ", " + self.y to string + ")";
    }
}

@entrypoint
fn main() {
    let p: Point = Point { 1, 2 };
    print(p to string);
}
