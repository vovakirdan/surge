fn join(...args: string) -> string {
    let mut out: string = "";
    let sep: string = "|";
    let mut i: int = 0;
    let n: int = args.__len() to int;
    while i < n {
        if i > 0 {
            out = out + sep;
        }
        let part: &string = args[i];
        out = out + part;
        i = i + 1;
    }
    return out;
}

