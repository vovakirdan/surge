@entrypoint
fn main() -> int {
    let a = task async {
        print("A1");
        checkpoint().await();
        print("A2");
        return 0;
    };
    let b = task async {
        print("B1");
        checkpoint().await();
        print("B2");
        return 0;
    };
    let _ = a.await();
    let _ = b.await();
    return 0;
}
