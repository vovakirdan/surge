@entrypoint
fn main() -> int {
    let x: int = compare (async {
        let a = 1;
        checkpoint().await();
        let b = a + 1;
        checkpoint().await();
        return b + 1;
    }).await() {
        Success(v) => v;
        Cancelled() => 0;
    };
    print(x to string);
    return 0;
}
