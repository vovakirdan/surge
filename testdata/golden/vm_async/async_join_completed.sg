@entrypoint
fn main() -> int {
    let t = task async {
        return 9;
    };
    let t1 = t.clone();

    let r1: int = compare t.await() {
        Success(v) => v;
        Cancelled() => 0;
    };
    let r2: int = compare t1.await() {
        Success(v) => v;
        Cancelled() => 0;
    };

    print("R1=" + (r1 to string));
    print("R2=" + (r2 to string));
    return 0;
}
