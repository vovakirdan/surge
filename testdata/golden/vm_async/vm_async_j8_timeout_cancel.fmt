@entrypoint
fn main() -> int {
    let worker = task async {
        sleep(100).await();
        print("worker done");
        return 1;
    };

    let r = timeout(worker, 10);
    compare r {
        Success(v) => print("success=" + (v to string));
        Cancelled() => print("timeout");
    };

    let guard = task async {
        sleep(200).await();
        return 0;
    };
    let _ = guard.await();

    return 0;
}
