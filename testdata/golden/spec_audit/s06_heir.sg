// ยง6.3 Inheritance Checking (heir) Audit

tag SomeInt(int);

type Base = { x: int };

type Child = Base : { y: int };

type AliasInt = int;

type Mixed = SomeInt(int) | int;

@entrypoint
fn main() {
    print("Child heir Base = " + (Child heir Base) to string);
    print("Base heir Child = " + (Base heir Child) to string);
    print("AliasInt heir int = " + (AliasInt heir int) to string);
    print("own Child heir Base = " + (own Child heir Base) to string);
    print("int heir Mixed = " + (int heir Mixed) to string);
    print("nothing heir Mixed = " + (nothing heir Mixed) to string);

    print("ยง6.3 heir operator: PASS");
}
