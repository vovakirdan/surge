type Point = {
    x: int,
    y: int,
};

extern<Point> {
    fn __add(self: &Point, other: &Point) -> Point {
        return { x: self.x + other.x, y: self.y + other.y };
    }

    @overload fn __add(self: Point, other: Point) -> Point {
        return { x: self.x + other.x, y: self.y + other.y };
    }
}

fn main() {
    let p1: Point = { x: 1, y: 2 };
    let p2: Point = { x: 3, y: 4 };
    let _ = p1 + p2;
    let _ = p1.x + p2.x;
}
