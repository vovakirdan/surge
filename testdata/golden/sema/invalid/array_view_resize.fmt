// Array views are not resizable: push/pop/reserve are rejected.
fn push_view() -> nothing {
    let mut a: int[] = [1, 2, 3];
    let mut v = a[[1..3]];
    v.push(4);
}

fn pop_view() -> nothing {
    let mut a: int[] = [1, 2, 3];
    let mut v = a[[1..3]];
    let _ = v.pop();
}

fn reserve_view() -> nothing {
    let mut a: int[] = [1, 2, 3];
    let mut v = a[[1..3]];
    v.reserve(10:uint);
}

fn array_push_view() -> nothing {
    let mut a: int[] = [1, 2, 3];
    let mut v = a[[1..3]];
    array_push(&mut v, 5);
}

fn array_pop_view() -> nothing {
    let mut a: int[] = [1, 2, 3];
    let mut v = a[[1..3]];
    let _ = array_pop(&mut v);
}

fn array_reserve_view() -> nothing {
    let mut a: int[] = [1, 2, 3];
    let mut v = a[[1..3]];
    array_reserve(&mut v, 8:uint);
}

fn rt_array_push_view() -> nothing {
    let mut a: int[] = [1, 2, 3];
    let mut v = a[[1..3]];
    rt_array_push(&mut v, 6);
}
