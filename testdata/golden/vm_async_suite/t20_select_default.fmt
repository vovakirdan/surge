@entrypoint
fn main() -> int {
    let r = (async {
        let ch = make_channel::<int>(0);
        let v = select {
            ch.recv() => 1;
            default => 2;
        };
        return v;
    }).await();

    compare r {
        Success(v) => print("v=" + (v to string));
        Cancelled() => print("cancelled");
    };
    return 0;
}
