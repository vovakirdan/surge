async fn run() -> int {
    let worker = spawn async {
        sleep(3).await();
        return 3;
    };

    let r = timeout(worker, 10);
    compare r {
        Success(v) => print("ok=" + (v to string));
        Cancelled() => print("result=timeout");
    };
    return 0;
}

@entrypoint
fn main() -> int {
    let exit_code: int = compare run().await() {
        Success(v) => v;
        Cancelled() => 1;
    };
    return exit_code;
}
