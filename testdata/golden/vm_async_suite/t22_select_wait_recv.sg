@entrypoint
fn main() -> int {
    let r = (async {
        let ch = make_channel<int>(0);
        let prod = spawn async {
            sleep(5).await();
            ch.send(7);
            return 0;
        };

        let v = select {
            ch.recv() => 7;
            sleep(100).await() => 0;
        };

        let _ = prod.await();
        return v;
    }).await();

    compare r {
        Success(v) => print("v=" + (v to string)),
        Cancelled => print("cancelled"),
    }
    return 0;
}
