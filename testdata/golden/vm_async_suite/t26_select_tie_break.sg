@entrypoint
fn main() -> int {
    let r = (async {
        let v = select {
            sleep(10).await() => 1;
            sleep(10).await() => 2;
        };
        return v;
    }).await();

    compare r {
        Success(v) => print("v=" + (v to string)),
        Cancelled => print("cancelled"),
    }
    return 0;
}
