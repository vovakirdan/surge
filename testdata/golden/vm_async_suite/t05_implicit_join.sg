@entrypoint
fn main() -> int {
    let r = (async {
        let _child = task async {
            checkpoint().await();
            print("child");
            return 0;
        };
        return 5;
    }).await();

    compare r {
        Success(v) => print("parent=" + (v to string));
        Cancelled() => print("parent=cancelled");
    };
    return 0;
}
