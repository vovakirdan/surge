type Point = {
    x: int,
    y: int,
};

extern<Point> {
    fn __add(self: &Point, other: &Point) -> Point {
        return { x: self.x + other.x, y: self.y + other.y };
    }

    fn __neg(self: &Point) -> Point {
        return { x: -self.x, y: -self.y };
    }

    fn __to(self: &Point, target: int) -> int {
        return self.x + self.y;
    }
}

