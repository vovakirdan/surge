@entrypoint
fn main() -> int {
    for i in 1..100 {
        let fizz = i % 3 == 0;
        let buzz = i % 5 == 0;
        if fizz && buzz {
            print("FizzBuzz");
        } else if fizz {
            print("Fizz");
        } else if buzz {
            print("Buzz");
        } else {
            print(i to string);
        }
    }
    return 0;
}
